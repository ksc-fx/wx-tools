page {
    background-color: #413735;
}
.container {
    width: 100%;
    height: 100%;
}
.move-container {
    width: 100%;
    height: 100%;
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
    align-items: flex-start;
    box-sizing: border-box;
    box-shadow: inset 0 1px 0 0 rgba(94, 86, 83, 0.3);
}
.tool-item {
    width: 33.33%;
    height: 33.33%;
    box-sizing: border-box;
}
.tool-item {
    box-shadow: -1px 1px 0 0 rgba(94, 86, 83, 0.3);
    display: flex;
    justify-content: center;
}
navigator {
    display: flex;
    width: 100%;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}
.tool-item__img {
    color: #fff;
    margin-bottom: 10rpx;
    font-size: 64rpx;
}
.tool-item__text {
    color: #fff;
    display: block;
    font-size: 24rpx;
    text-align: center;
}
